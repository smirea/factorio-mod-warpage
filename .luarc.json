{
  "runtime.version": "Lua 5.2",
  "workspace.checkThirdParty": false,
  "workspace.library": [
    ".luals/factorio"
  ],
  "workspace.ignoreDir": [
    ".git",
    ".luals",
    ".tmp",
    "research"
  ],
  "diagnostics.globals": [
    "commands",
    "data",
    "defines",
    "game",
    "log",
    "mods",
    "remote",
    "rendering",
    "script",
    "serpent",
    "settings",
    "storage"
  ],
  "diagnostics.groupFileStatus": {
    "global": "Any",
    "luadoc": "Any",
    "strict": "Any",
    "type-check": "Any",
    "unbalanced": "Any"
  },
  "diagnostics.groupSeverity": {
    "strict": "Warning",
    "type-check": "Error"
  },
  "type.inferParamType": true,
  "type.checkTableShape": true,
  "type.weakNilCheck": false,
  "type.weakUnionCheck": false
}
